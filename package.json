{
  "name": "voile",
  "version": "0.1.1",
  "description": "Image gallery and comic book reader",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"webpack --config webpack.dev.js\" \"node ./server\"",
    "dev:client": "cross-env NODE_ENV=development webpack --config webpack.dev.js",
    "test": "find . -not -path \"*node_modules*\" -name \"*test\" | xargs mocha",
    "coverage": "find . -not -path \"*node_modules*\" -name \"*test\" | xargs nyc --reporter=html --reporter=text mocha",
    "debug-test": "find . -not -path \"*node_modules*\" -name \"*test\" | xargs mocha --inspect-brk --timeout 999999",
    "build": "webpack --config webpack.prod.js",
    "serve": "node ./server",
    "prod": "npm run build && npm run serve"
  },
  "keywords": [],
  "author": "ktucalvin",
  "license": "MIT",
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "no-descending-specificity": null
    }
  },
  "dependencies": {
    "compressible": "^2.0.18",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "fuse.js": "^3.6.1",
    "koa": "^2.11.0",
    "koa-compress": "^3.1.0",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-helmet": "^5.2.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "mocha": "^7.1.2",
    "mysql": "^2.18.1",
    "promise-mysql": "^4.1.3",
    "react-hot-loader": "^4.12.21",
    "sharp": "^0.23.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "dirty-chai": "^2.0.1",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.2",
    "mock-require": "^3.0.3",
    "nyc": "^14.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.5.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.3.3",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^19.0.0",
    "terser-webpack-plugin": "^2.3.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-plugin-serve": "^0.12.1"
  }
}
