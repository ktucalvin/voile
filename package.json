{
  "name": "voile",
  "version": "0.2.0",
  "description": "SPA built with Koa and React for serving image galleries",
  "main": "index.js",
  "private": true,
  "keywords": [],
  "author": "ktucalvin",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "cross-env NODE_ENV=development webpack --config webpack.dev.js",
    "dev:server": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register --files -P ./src/server/tsconfig.json ./src/server",
    "test": "jest",
    "coverage": "jest --coverage",
    "clean": "rimraf dist/client dist/server",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:client": "webpack --config webpack.prod.js",
    "build:server": "tsc --listEmittedFiles -p ./src/server",
    "serve": "node ./dist/server",
    "prod": "npm run clean && npm run build && npm run serve"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "ignoreFiles": [
      "**/*.tsx",
      "**/*.js"
    ],
    "rules": {
      "no-descending-specificity": null
    }
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "engine": "standardx",
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "_moduleAliases": {
    "@server": "dist/server",
    "@client": "dist/client"
  },
  "dependencies": {
    "compressible": "^2.0.18",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "fuse.js": "^6.4.6",
    "koa": "^2.13.1",
    "koa-compress": "^5.0.1",
    "koa-conditional-get": "^3.0.0",
    "koa-etag": "^4.0.0",
    "koa-helmet": "^6.0.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^10.0.0",
    "koa-send": "^5.0.1",
    "koa-static": "^5.0.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "postcss": "^8.2.7",
    "qs": "^6.9.6",
    "react-hot-loader": "^4.13.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.27.2",
    "typeorm": "^0.2.31",
    "typescript": "^4.2.3",
    "webpack": "^5.24.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@shopify/jest-koa-mocks": "^2.3.2",
    "@types/compressible": "^2.0.0",
    "@types/jest": "^26.0.20",
    "@types/koa": "^2.13.1",
    "@types/koa-compress": "^4.0.1",
    "@types/koa-conditional-get": "^2.0.0",
    "@types/koa-etag": "^3.0.0",
    "@types/koa-helmet": "^6.0.2",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/sharp": "^0.27.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.1.1",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.1",
    "html-webpack-plugin": "^5.2.0",
    "jest": "^26.6.3",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "mini-css-extract-plugin": "^1.3.9",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^5.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.2",
    "standardx": "^7.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.11.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.5.3",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2",
    "webpack-plugin-serve": "^1.3.0"
  }
}
