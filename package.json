    "dirty-chai": "^2.0.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "koa-webpack": "^5.2.4",
    "less": "^3.9.0",
