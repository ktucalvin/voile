{
  "name": "voile",
  "version": "0.2.0",
  "description": "SPA built with Koa and React for serving image galleries",
  "main": "index.js",
  "private": true,
  "keywords": [],
  "author": "ktucalvin",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"webpack --config webpack.dev.js\" \"ts-node ./server\"",
    "dev:client": "cross-env NODE_ENV=development webpack --config webpack.dev.js",
    "test": "find ./server -not -path \"*node_modules*\" -name \"*test.ts\" | xargs ts-mocha",
    "coverage": "find ./server -not -path \"*node_modules*\" -name \"*test.ts\" | xargs nyc --reporter=html --reporter=text ts-mocha",
    "debug-test": "find . -not -path \"*node_modules*\" -name \"*test.ts\" | xargs ts-mocha --inspect-brk --timeout 999999",
    "build": "webpack --config webpack.prod.js",
    "serve": "ts-node ./server",
    "prod": "npm run build && npm run serve"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "no-descending-specificity": null
    }
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "engine": "standardx",
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "dependencies": {
    "compressible": "^2.0.18",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "fuse.js": "^3.6.1",
    "koa": "^2.11.0",
    "koa-compress": "^3.1.0",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-helmet": "^5.2.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "mocha": "^7.1.2",
    "mysql2": "^2.1.0",
    "react-hot-loader": "^4.12.21",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.23.4",
    "ts-node": "^8.10.1",
    "typeorm": "^0.2.24",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@types/chai": "^4.2.11",
    "@types/compressible": "^2.0.0",
    "@types/dirty-chai": "^2.0.2",
    "@types/koa": "^2.11.3",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-conditional-get": "^2.0.0",
    "@types/koa-etag": "^3.0.0",
    "@types/koa-helmet": "^5.2.0",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@types/mocha": "^7.0.2",
    "@types/mock-require": "^2.0.0",
    "@types/node": "^14.0.1",
    "@types/sharp": "^0.25.0",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "dirty-chai": "^2.0.1",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.2",
    "nyc": "^14.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.5.0",
    "standardx": "^5.0.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.3.3",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^19.0.0",
    "terser-webpack-plugin": "^2.3.6",
    "ts-mocha": "^7.0.0",
    "ts-mock-imports": "^1.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-plugin-serve": "^0.12.1"
  }
}
