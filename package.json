{
  "name": "voile",
  "version": "0.1.1",
  "description": "Image gallery and comic book reader",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node index.js",
    "test": "find . -not -path \"*node_modules*\" -name \"*test\" | xargs mocha",
    "coverage": "find . -not -path \"*node_modules*\" -name \"*test\" | xargs nyc --reporter=html --reporter=text mocha",
    "debug-test": "find . -not -path \"*node_modules*\" -name \"*test\" | xargs mocha --inspect-brk --timeout 999999",
    "build": "webpack --config webpack.prod.js"
  },
  "keywords": [],
  "author": "ktucalvin",
  "license": "MIT",
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "no-descending-specificity": null
    }
  },
  "dependencies": {
    "compressible": "^2.0.17",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "fuse.js": "^3.4.5",
    "koa": "^2.7.0",
    "koa-compress": "^3.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "react-hot-loader": "^4.12.10",
    "sharp": "^0.22.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
